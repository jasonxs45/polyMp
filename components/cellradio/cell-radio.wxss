.cell-radio{
  width: 100%;
  height: inherit;
  line-height: inherit;
  background: inherit;
  display: inline-block;
  border-radius: inherit;
  border:none;
  background: #f5f5f5;
  transition: all .2s;
}
.cell-radio.checked{
  color: #edc27b;
  background: #4d4d4d;
}
.cell-radio.bg{
  position: relative;
  background: #f5f5f5;
}
.cell-radio.bg.checked{
  background: url('data:image/png;base64,') center/100% 100% no-repeat;
  color: #333;
}