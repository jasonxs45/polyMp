page{
  background: #fff;
}
.regist{
  min-height: 100vh;
  position: relative;
  width: 100%;
  padding: 10rpx 0 200rpx;
}
.title{
  text-align: center;
}
.form{
  padding: 30rpx;
}
.form-bar{
  display: block;
  margin-top: 30rpx;
}
.fh{
  font-size:24rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:34rpx;
}
.fb{
  font-size: 30rpx;
  margin-top: 30rpx;
}
.clearable-input{
  display: flex;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #F5F5F5;
  border-radius: 4rpx;
}
.input{
  width: 100%;
  height: 100%;
  color: #4d4d4d;
}
.btns{
  text-align: center;
  width: 100%;
  height: 200rpx;
  position: relative;
  margin-top: -200rpx;
  padding: 50rpx 30rpx;
}
.btn{
  font-size:34rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-weight: 600;
}