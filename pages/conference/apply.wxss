.conForm {
  min-height: 100vh;
  padding-bottom: 200rpx;
}
.electronic {
  background: #fff;
  margin-bottom: 20rpx;
}
.form {
  padding: 30rpx 30rpx 40rpx;
}
.form-bar {
  display: block;
  margin-top: 40rpx;
}
.fh {
  font-size: 24rpx;
  color: #333;
}
.fh text {
  font-size: 20rpx;
  color: #666;
}
.fb {
  font-size: 26rpx;
  margin-top: 20rpx;
}
.cell-select .text {
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 4rpx;
  padding: 0 10rpx;
  min-width: 160rpx;
  text-align: center;
  margin-right: 30rpx;
  border: none;
  background: #f5f5f5;
}
.clearable-input {
  display: flex;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
}
.clearable-textarea {
  display: block;
  width: 100%;
  padding: 20rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
  position: relative;
  z-index: 5;
}
.clearable-textarea textarea {
  width: 100%;
  height: 180rpx;
  font-size: 26rpx;
  line-height: 32rpx;
}
.input {
  width: 100%;
  height: 100%;
}
.btns {
  text-align: center;
  width: 100%;
  height: 200rpx;
  padding: 50rpx 30rpx;
  position: relative;
  margin-top: -200rpx;
}
.color9 {
  color: #999;
  line-height: inherit;
  padding: 0 15rpx;
}
.con_time {
  padding: 28rpx 0 30rpx 0;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #eee;
}
.cell-group{
  width: 300rpx;
  text-align: right;
}
.noon {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 2px;
  font-weight: 500;
  text-align: center;
  background: #4d4d4d;
  color: #fff;
  border: 2rpx solid #4d4d4d;
  margin-left: 30rpx;
}
.need-to-know{
  color: #666;
  font-size: 0;
  margin-top: 30rpx;
}
.need-to-know .radio-wrapper{
  display: inline;
}
.need-to-know .radio-wrapper text{
  vertical-align: middle;
  font-size: 24rpx;
}
.need-to-know checkbox .wx-checkbox-input{
  width: 24rpx !important;
  height: 24rpx !important;
  margin-right: 10rpx;
  display:inline-block;
}
.need-to-know .nav{
  display: inline;
  text-decoration: underline;
  vertical-align: middle;
  font-size: 24rpx;
}