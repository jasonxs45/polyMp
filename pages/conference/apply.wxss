.conForm {
  min-height: 100vh;
  padding-bottom: 200rpx;
}

.electronic {
  background: #fff;
  margin-bottom: 20rpx;
}

.form {
  padding: 30rpx 30rpx 40rpx;
}

.form-bar {
  display: block;
  margin-top: 40rpx;
}

.fh {
  font-size: 24rpx;
  color: #333;
}

.fh text {
  font-size: 20rpx;
  color: #666;
}

.fb {
  font-size: 26rpx;
  margin-top: 20rpx;
}

.cell-select .text {
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 4rpx;
  padding: 0 10rpx;
  min-width: 160rpx;
  text-align: center;
  margin-right: 30rpx;
  border: none;
  background: #f5f5f5;
}

.clearable-input {
  display: flex;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
}

.clearable-textarea {
  display: block;
  width: 100%;
  padding: 20rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
  position: relative;
  z-index: 5;
}

.clearable-textarea textarea {
  width: 100%;
  height: 240rpx;
  font-size: 26rpx;
  line-height: 32rpx;
}

.input {
  width: 100%;
  height: 100%;
}

.btns {
  text-align: center;
  width: 100%;
  height: 200rpx;
  padding: 50rpx 30rpx;
  position: relative;
  margin-top: -200rpx;
}

.con_time {
  color: #333;
  text-align: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}

.con_time text {
  font-size: 26rpx;
  text-align: center;
  line-height: 60rpx;
}

.noon {
  width: 120rpx;
  border-radius: 2px;
  border: 2rpx solid #eee;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #999;
}

.noon.on {
  background: #4d4d4d;
  color: #fff;
  border: 2rpx solid #4d4d4d;
}

.timeMain {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  z-index: 99;
}

.timeMain_bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.8);
}

.timeContent {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 80vh;
  padding: 0rpx 0 108rpx 0;
  background: #f5f5f5;
  border-radius: 20rpx 20rpx 0px 0px;
}

.timeContent_c {
  height: 100%;
  background: #fff;
  border-radius: 4px 4px 0px 0px;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.timeContent_c .up, .timeContent_c .down {
  height: 50rpx;
  line-height: 50rpx;
  width: 100%;
  text-align: center;
  color: #ddd;
  font-size: 30rpx;
}

.timeContent_c .content {
  padding: 30rpx;
  height: calc(100% - 100rpx);
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}

.timeContent .con_time {
  padding: 28rpx 0 30rpx 0;
  border-bottom: 2rpx solid #eee;
}

.timeBtn {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background: #fff;
  font-size: 34rpx;
  font-weight: 500;
  color: #333;
}

.color9 {
  color: #999;
  line-height: inherit;
  padding: 0 15rpx;
}
.need-to-know{
  color: #666;
  font-size: 0;
  margin-top: 30rpx;
}
.need-to-know .radio-wrapper{
  display: inline;
}
.need-to-know .radio-wrapper text{
  vertical-align: middle;
  font-size: 24rpx;
}
.need-to-know checkbox .wx-checkbox-input{
  position: relative;
  width: 24rpx !important;
  height: 24rpx !important;
  margin-right: 10rpx;
  display:inline-block;
  box-sizing: border-box;
}
.need-to-know checkbox[checked] .wx-checkbox-input{
  background: #666;
}
.need-to-know checkbox[checked] .wx-checkbox-input::before{
  color: transparent;
}
.need-to-know .nav{
  display: inline;
  text-decoration: underline;
  vertical-align: middle;
  font-size: 24rpx;
}