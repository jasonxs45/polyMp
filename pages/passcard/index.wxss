page {
  background: #f2f2f2;
}

.electronic {
  background: #fff;
  margin-bottom: 20rpx;
}

.form {
  padding: 30rpx 30rpx 40rpx;
}

.electronic2 .form {
  padding: 1px 30rpx 30rpx;
}

.form-bar {
  display: block;
  margin-top: 40rpx;
}

.fh {
  font-size: 24rpx;
  color: #333;
}

.fh text {
  font-size: 20rpx;
  color: #666;
}

.fb {
  font-size: 26rpx;
  margin-top: 20rpx;
}

.cell-select .text {
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 4rpx;
  padding: 0 10rpx;
  min-width: 160rpx;
  text-align: center;
  margin-right: 30rpx;
  border: none;
  background: #f5f5f5;
}

.clearable-input {
  display: flex;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
}

.clearable-textarea {
  display: block;
  width: 100%;
  padding: 20rpx;
  background: #f5f5f5;
  border-radius: 4rpx;
}

.clearable-textarea textarea {
  width: 100%;
  height: 240rpx;
  font-size: 26rpx;
  line-height: 32rpx;
}

.input {
  width: 100%;
  height: 100%;
}

.picker {
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 4rpx;
  padding: 0 10rpx;
  width: 100%;
  min-width: 160rpx;
  border: none;
  background: #f5f5f5;
}

.imgrow {
  flex-wrap: wrap;
  margin: 30rpx -10rpx
}
.imgrow .cell{
  width: 20%;
  margin: 10rpx 0;
  position: relative;
}
.imgrow .cell .del{
  position: absolute;
  top:-8px;
  right:-3px;
}
.imgrow .cell image{
  width: 120rpx;
  height: 120rpx;
  margin: 0 auto;
  border-radius: 4rpx;
}

.imgrow .cell .icon {
  font-size: 60rpx;
  color: #999;
  width: 120rpx;
  height: 120rpx;
  margin: 0 auto;
  line-height: 120rpx;
  text-align: center;
  border:1px dashed #999;
  border-radius: 4rpx;
}

.btns {
  text-align: center;
  width: 100%;
  height: 200rpx;
  padding: 50rpx 30rpx;
}

.btn {
  font-size: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
}
