page{
  background: #f2f2f2;
}
.section, .pad30{
  background: #fff;
  padding: 30rpx;
}
.pad30 {
  background: none;
}
.swiper{
  width: 100%;
  height: 320rpx;
}
.swiper .nav,
.swiper .img{
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}
.entries{
  justify-content: space-between;
  margin: 50rpx 0;
}
.entry{
  display: inline-block;
  text-align: center;
}
.entry .img{
  width:98rpx;
  height: 96rpx;
  margin: 0 auto;
}
.entry .text{
  color: #333;
  font-size: 24rpx;
  margin-top: 15rpx;
  letter-spacing: -1rpx;
  word-spacing: -1rpx;
}
.title{
  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
}
.title .text{
  font-size: 34rpx;
  color:#333;
  font-weight: bold;
}
.title .nav{
  color: #999;
  font-size: 24rpx;
}
.activity-nav {
  margin-top: 30rpx;
  background: #fff;
  border-radius: 8rpx;
}
.activity-nav .img{
  width: 100%;
  height: 320rpx;
  border-radius: 8rpx;
}
.list-wrapper{
  display: flex;
  flex-wrap: wrap;
  margin: 30rpx -15rpx;
}
.cell {
  width: 330rpx;
  height: 530rpx;
  margin: 15rpx;
  background: #fff;
  overflow: hidden;
  border-radius: 8rpx;
  box-shadow: 0 3rpx 3rpx 0 rgba(0,0,0,.2);
}

.cell .img{
  width: 330rpx;
  height: 330rpx;
  position: relative;
}
.cell .img .disable{
  position: absolute;
  top:0;
  right:0;
  background: rgba(0,0,0,.6);
  z-index: 1;
  width: 100%;
  height: 100%;
  color: #fff;
  line-height: 314rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: 600;
  text-shadow: 1px 1px 3px rgba(0,0,0,.7);
}
.cell .detail{
  padding:30rpx;
}
.cell .tit{
  font-size: 28rpx;
  color: #333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 40rpx;
  height: 80rpx;
}
.cell .info{
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-top: 30rpx;
}
.cell .cost{
  font-size: 28rpx;
  font-weight: 500;
  color: #333;
}
.cell .cost text{
  color: #666;
  font-size: 16rpx;
  font-weight: 400;
}
.cell .btn{
  width: 118rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin: 0;
  font-size: 28rpx;
  border-radius: 4rpx; 
  color: #fff !important;
}
.bottom-line{
  justify-content: space-between;
  align-items: center;
  color: #999;
  padding:0 30rpx 30rpx;
  font-size: 24rpx;
}
.bottom-line .text{
    margin:0 20rpx;
}
.bottom-line::before,
.bottom-line::after{
  content: '';
  height: 1rpx;
  background: #ddd;
  flex: 1;
  display: block;
}
.ad-wrapper,.rp-wrapper{
  position: fixed;
  width: 100%;
  height: 100%;
  top:0;
  left:0;
  z-index: 198801213;
}
.ad-wrapper .bg,
.rp-wrapper .bg{
  background: rgba(0,0,0,.8);
  width: 100%;
  height: 100%;
}
.ad-wrapper .ad,
.rp-wrapper .ad{
  position: absolute;
  top:50%;
  left:50%;
  z-index: 1;
  text-align: center;
  transform: translate(-50%, -50%);
}
.adimg{
}
.rp-wrapper .adimg{
  width: 360rpx;
  margin-bottom: 60rpx;
}
.rp-wrapper .adimg image{
  width: 100%;
}
.adimg.navigator-hover{
  opacity: 1;
}