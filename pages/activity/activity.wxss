.actTop{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 96rpx;
  box-shadow:0px 2px 4px 0px rgba(0,0,0,0.03),0px -1px 0px 0px rgba(0,0,0,0.05);
}
.text{
  font-size:28rpx;
  font-family:PingFang-TC-Light;
  font-weight:300;
  color:#666666;
  text-align: center;
  height: 96rpx;
  line-height: 96rpx;
}
.text.on{
  font-weight:700;
  color: #333333;
}
.actMain{
  padding-top: 96rpx;
  height: 100vh;
}
.swiper{
  width: 100%;
  height: 100%;
  overflow: hidden;
  padding: 40rpx 0;
}
swiper-item {
  overflow-x:hidden;
  overflow-y:auto;
  -webkit-overflow-scrolling:touch;
}
.acList{
  display: block;
  width: 100%;
  height: 320rpx;
  margin-bottom: 40rpx;
  overflow: hidden;
  border-radius: 8rpx;
  padding: 0 30rpx;
}
.acList image{ 
  width: 100%;
  height: 100%;
  border-radius: inherit;
}