.regist{
  height: 100vh;
  position: relative;
  width: 100%;
  padding: 0 0 200rpx;
}
.content{
  height: 100%;
}
.title{
  text-align: center;
}
.logo{
  width: 318rpx;
  margin: 60rpx auto 0;
  display: block;
}
.form{
  padding: 30rpx;
}
.form-bar{
  display: block;
  margin-top: 30rpx;
}
.fh{
  font-size: 26rpx;
}
.fb{
  font-size: 30rpx;
  margin-top: 30rpx;
}
.cell-select .text{
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 4rpx;
  padding:0 10rpx;
  min-width: 160rpx;
  text-align: center;
  margin-right: 30rpx;
  border:none;
  background: #f5f5f5;
}
.clearable-input{
  display: flex;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #F5F5F5;
  border-radius: 4rpx;
}
.input{
  width: 100%;
  height: 100%;
}
.get-code{
  flex: 0 0 180rpx;
  margin-left: 10rpx;
}
.get-code-btn{
  font-size: 24rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding:0 10rpx;
  width: auto;
  border-radius: 4rpx;
}
.btns{
  text-align: center;
  width: 100%;
  height: 200rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 50rpx 30rpx;
}
.btn{
  font-size:30rpx;
  height: 100rpx;
  line-height: 100rpx;
}