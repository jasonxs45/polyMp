page{
  background: #f2f2f2;
}
.section, .pad30{
  background: #fff;
  padding: 30rpx;
}
.pad30 {
  background: none;
}
.swiper{
  width: 100%;
  height: 320rpx;
}
.swiper .nav,
.swiper .img{
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}
.entries{
  margin: 30rpx -15rpx 10rpx;
  flex-wrap: wrap;
}
.entries .cell{
  width: 20%;
  margin-bottom: 30rpx;
  text-align: center;
}
.entry{
  display: inline-block;
}
.entry .img{
  width:98rpx;
  height: 96rpx;
  margin: 0 auto;
}
.entry .text{
  color: #333;
  font-size: 24rpx;
  margin-top: 15rpx;
  letter-spacing: -1rpx;
  word-spacing: -1rpx;
}