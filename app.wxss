/**app.wxss**/
view, cover-view, scroll-view, picker-view, swiper, swiper-item, navigator, image, input, text, textarea{
  box-sizing: border-box;
  padding:0;
  margin:0;
  line-height: 1;
}
image{
  display: block;
}
.navigator-hover{
  background: none;
}
.form-bar,.flex{
  display: flex;
}
@keyframes slide-up {
  0% {transform: translateY(100%)}
  100% {transform: none}
}
.slide-up{
  animation-name: slide-up
}
.slide-up-reverse{
  animation-name: slide-up;
  animation-direction: reverse;
}
.animation{
  animation-duration: 200ms;
}
/* 标签形式的选择 */
.cell-radio{
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 4rpx;
  min-width: 160rpx;
  text-align: center;
  margin-right: 30rpx;
  border:none;
  font-size: 26rpx;  
}
